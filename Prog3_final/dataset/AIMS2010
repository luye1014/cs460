CREATE TABLE AIMS2011 (
Fiscal_Year integer,
State varchar2(10),
County varchar2(40),
LEA_EntityID varchar2(10),
LEA_CTDSnum varchar2(15),
LEA_Name varchar2(100),
School_EntityID varchar2(10),
School_CTDSnum varchar2(15),
School_Name varchar2(80),
S_charter varchar2(1),
Math_mean integer,
Math_PercFallsFarBelow integer,
Math_PercApproaches integer,
Math_PercMeets integer,
Math_PercExceeds integer,
Math_PercPassing integer,
Reading_mean integer,
Reading_PercFallsFarBelow integer,
Reading_PercApproaches integer,
Reading_PercMeets integer,
Reading_PercExceeds integer,
Reading_PercPassing integer,
Writing_mean integer,
Writing_PercFallsFarBelow integer,
Writing_PercApproaches integer,
Writing_PercMeets integer,
Writing_PercExceeds integer,
Writing_PercPassing integer,
Science_mean integer,
Science_PercFallsFarBelow integer,
Science_PercApproaches integer,
Science_PercMeets integer,
Science_PercExceeds integer,
Science_PercPassing integer,
primary key (School_EntityID)
);



LOAD DATA
INFILE '2010.csv'
INTO TABLE AIMS2010
 fields terminated by "," optionally enclosed by '"'		  
 (Fiscal_Year integer,
State varchar(7),
County varchar(7),
LEA_EntityID integer,
LEA_CTDSnum integer,
LEA_Name varchar(80),
School_EntityID integer,
